        <element label="Story Number" max_length="40" name="story_number" type="string"/>
        <element label="Story Owner" max_length="40" name="story_owner" type="string"/>
        <element label="Update Set(s)" max_length="4000" name="update_set_s" reference="sys_update_set" type="glide_list"/>
